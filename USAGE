
==========================
USAGE ON MAC OS 
==========================

# google search homebrew macos to install homebrew
brew install --formula python
brew install --cask mactex-no-gui

# install python dependencies
pip3 install -r pip_requirements.txt

# get account on rebrickable.com and obtain api key
echo 'api_key: <my_api_key>' > rebrick_api_key.yml
# example hex key (not a real one)
echo 'api_key: db161ef60f45e5d76cf92b6a6d6a3f6b' > rebrick_api_key.yml 

# lookup the set data and save to CSV
./lookup_set_rebrick.py example_sets.csv

# create TeX file from set ID list
./super_make_set_labels.py example_sets.csv

# create PDF from TeX file
latexmk -cd -xelatex -interaction=nonstopmode -halt-on-error -file-line-error \
  -outdir=output/super_make output/super_make/labels-example_sets.tex

# open the PDF file in Preview.app
open output/super_make/labels-example_sets.pdf
